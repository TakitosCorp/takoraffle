<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <ApplicationIcon>wwwroot\favicon.ico</ApplicationIcon>
        <Title>TakoRaffle</Title>
        <Authors>AlexDeveloperUwU</Authors>
        <Description>A simple website for a raffle organized by GalaYak</Description>
        <PackageProjectUrl>https://github.com/TakitosCorp/takoraffle</PackageProjectUrl>
        <PackageIcon>favicon.png</PackageIcon>
        <RepositoryUrl>https://github.com/TakitosCorp/takoraffle</RepositoryUrl>
        <Company>AlexDeveloperUwU</Company>
        <Product>TakoRaffle</Product>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.4" />
      <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="9.0.4" />
    </ItemGroup>

</Project>
